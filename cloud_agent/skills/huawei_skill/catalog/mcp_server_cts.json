    {
      "name": "DeleteTracker",
      "description": "云审计服务目前仅支持删除已创建的数据类追踪器。删除追踪器对已有的操作记录没有影响,当您重新开通云审计服务后,依旧可以查看已有的操作记录。",
      "inputSchema": {
        "type": "object",
        "properties": {
          "tracker_type": {
            "example": "data",
            "description": "标识追踪器类型。",
            "type": "string",
            "maxLength": 32,
            "minLength": 1,
            "enum": [
              "data",
              "system"
            ],
            "in": "query"
          },
          "tracker_name": {
            "example": "system",
            "description": "标识追踪器名称。",
            "type": "string",
            "maxLength": 64,
            "minLength": 1,
            "in": "query"
          },
          "project_id": {
            "example": "2b31ed520xxxxxxebedb6e57xxxxxxxx",
            "description": "项目ID,参见《云审计服务API参考》“获取账号ID和项目ID”章节。",
            "type": "string",
            "maxLength": 40,
            "minLength": 15,
            "in": "path"
          }
        },
        "required": [
          "project_id"
        ]
      }
    },
    {
      "name": "UpdateTracker",
      "description": "云审计服务支持修改已创建追踪器的配置项,包括OBS桶转储、关键事件通知、事件转储加密、通过LTS对管理类事件进行检索、事件文件完整性校验以及追踪器启停状态等相关参数,修改追踪器对已有的操作记录没有影响。修改追踪器完成后,系统立即以新的规则开始记录操作。",
      "inputSchema": {
        "type": "object",
        "properties": {
          "project_id": {
            "example": "2b31ed520xxxxxxebedb6e57xxxxxxxx",
            "description": "项目ID,参见《云审计服务API参考》“获取账号ID和项目ID”章节。",
            "type": "string",
            "maxLength": 40,
            "minLength": 15,
            "in": "path"
          },
          "tracker_type": {
            "description": "标识追踪器类型。",
            "type": "string",
            "enum": [
              "system",
              "data"
            ]
          },
          "tracker_name": {
            "type": "string",
            "description": "标识追踪器名称。",
            "maxLength": 64,
            "minLength": 1,
            "example": "data-tracker-name"
          },
          "agency_name": {
            "type": "string",
            "description": "云服务委托名称。",
            "maxLength": 32,
            "minLength": 1,
            "example": "cts_admin_trust",
            "enum": [
              "cts_admin_trust"
            ]
          },
          "status": {
            "type": "string",
            "description": "标识追踪器状态,该接口中可修改的状态包括正常(enabled)和停止(disabled)。如果选择修改状态为停止,则修改成功后追踪器停止记录事件。",
            "enum": [
              "enabled",
              "disabled"
            ],
            "example": "disabled"
          },
          "is_organization_tracker": {
            "type": "boolean",
            "description": "是否应用到我的组织。",
            "default": false
          },
          "management_event_selector": {
            "type": "object",
            "description": "管理类事件选择器。",
            "properties": {
              "exclude_service": {
                "type": "array",
                "description": "标识不转储的云服务名称。",
                "example": "",
                "items": {
                  "example": "KMS",
                  "type": "string",
                  "minLength": 1,
                  "maxLength": 20
                },
                "minItems": 0,
                "maxItems": 10
              }
            }
          },
          "is_lts_enabled": {
            "type": "boolean",
            "description": "是否打开事件分析。",
            "example": true
          },
          "obs_info": {
            "description": "转储桶配置",
            "properties": {
              "bucket_name": {
                "type": "string",
                "description": "标识OBS桶名称。由数字或字母开头,支持小写字母、数字、“-”、“.”,长度为3~63个字符。",
                "maxLength": 63,
                "minLength": 3,
                "example": "obs-f1da"
              },
              "file_prefix_name": {
                "type": "string",
                "description": "标识需要存储于OBS的日志文件前缀,0-9,a-z,A-Z,'-','.','_'长度为0~64字符。",
                "maxLength": 64,
                "minLength": 0,
                "example": "yO8Q"
              },
              "is_obs_created": {
                "type": "boolean",
                "description": "是否支持新建OBS桶。",
                "example": "true"
              },
              "bucket_lifecycle": {
                "type": "integer",
                "description": "标识配置桶内对象存储周期。",
                "minimum": 0,
                "maximum": 5000,
                "example": "30"
              },
              "compress_type": {
                "type": "string",
                "description": "压缩类型。包括不压缩(json),压缩(gzip)两种状态。默认为gzip格式。",
                "default": "gzip",
                "enum": [
                  "gzip",
                  "json"
                ]
              },
              "is_sort_by_service": {
                "type": "boolean",
                "description": "路径按云服务划分,打开后转储文件路径中将增加云服务名。默认为true。",
                "default": true
              }
            },
            "type": "object"
          },
          "is_support_trace_files_encryption": {
            "type": "boolean",
            "description": "事件文件转储加密功能开关。",
            "example": true
          },
          "kms_id": {
            "type": "string",
            "description": "事件文件转储加密所采用的秘钥id(从KMS获取)。",
            "maxLength": 200,
            "minLength": 0,
            "example": "13a4207c-7abe-4b68-8510-16b84c3b5504"
          },
          "is_support_validate": {
            "type": "boolean",
            "description": "事件文件转储时是否打开事件文件校验。",
            "example": true
          },
          "data_bucket": {
            "properties": {
              "data_bucket_name": {
                "type": "string",
                "maxLength": 63,
                "minLength": 3,
                "description": "数据类追踪器追踪对象的桶名。"
              },
              "data_event": {
                "type": "array",
                "maxItems": 2,
                "minItems": 1,
                "items": {
                  "type": "string",
                  "enum": [
                    "WRITE",
                    "READ"
                  ],
                  "example": "WRITE"
                },
                "description": "数据类追踪器追踪的操作类型。"
              }
            },
            "type": "object",
            "description": "追踪桶配置信息。"
          }
        },
        "required": [
          "project_id",
          "tracker_name",
          "tracker_type"
        ]
      }
    },
    {
      "name": "BatchDeleteResourceTags",
      "description": "批量删除CTS资源标签。",
      "inputSchema": {
        "type": "object",
        "properties": {
          "resource_type": {
            "description": "CTS服务的资源类型,目前仅支持cts-tracker。",
            "type": "string",
            "enum": [
              "cts-tracker"
            ],
            "in": "path"
          },
          "resource_id": {
            "description": "资源ID。",
            "type": "string",
            "maxLength": 350,
            "minLength": 0,
            "in": "path"
          },
          "project_id": {
            "example": "2b31ed520xxxxxxebedb6e57xxxxxxxx",
            "description": "项目ID,参见《云审计服务API参考》“获取账号ID和项目ID”章节。",
            "type": "string",
            "maxLength": 40,
            "minLength": 15,
            "in": "path"
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "object",
              "description": "标签列表。",
              "properties": {
                "key": {
                  "type": "string",
                  "description": "键。最大长度128个unicode字符。key不能为空。不能包含非打印字符ASCII(0-31),“=”,“*”,“<”,“>”,“\\”,“,”,“|”,“/”",
                  "minLength": 1,
                  "maxLength": 128
                },
                "value": {
                  "type": "string",
                  "description": "值。每个值最大长度255个unicode字符,删除时如果value有值按照key/value删除,如果value没值,则按照key删除,可以为空字符串。,不能包含非打印字符ASCII(0-31), “=”,“*”,“<”,“>”,“\\”,“,”,“|”,“/”",
                  "minLength": 0,
                  "maxLength": 255
                }
              }
            },
            "description": "标签列表。",
            "minItems": 0,
            "maxItems": 20
          }
        },
        "required": [
          "project_id",
          "resource_id",
          "resource_type"
        ]
      }
    },
    {
      "name": "ListTraceResources",
      "description": "查询事件的资源类型列表。",
      "inputSchema": {
        "type": "object",
        "properties": {
          "domain_id": {
            "description": "账户id,参见《云审计服务API参考》“获取账号ID和项目ID”章节。",
            "type": "string",
            "maxLength": 40,
            "minLength": 15,
            "in": "path"
          }
        },
        "required": [
          "domain_id"
        ]
      }
    },
    {
      "name": "ListTrackers",
      "description": "开通云审计服务成功后,您可以在追踪器信息页面查看追踪器的详细信息。详细信息主要包括追踪器名称,用于存储操作事件的OBS桶名称和OBS桶中的事件文件前缀。",
      "inputSchema": {
        "type": "object",
        "properties": {
          "tracker_type": {
            "example": "system",
            "description": "标识追踪器类型。",
            "type": "string",
            "maxLength": 32,
            "minLength": 1,
            "enum": [
              "system",
              "data"
            ],
            "in": "query"
          },
          "tracker_name": {
            "example": "system",
            "description": "标示追踪器名称。",
            "type": "string",
            "maxLength": 64,
            "minLength": 1,
            "in": "query"
          },
          "project_id": {
            "example": "2b31ed520xxxxxxebedb6e57xxxxxxxx",
            "description": "项目ID,参见《云审计服务API参考》“获取账号ID和项目ID”章节。",
            "type": "string",
            "maxLength": 40,
            "minLength": 15,
            "in": "path"
          }
        },
        "required": [
          "project_id"
        ]
      }
    },
    {
      "name": "ListTraces",
      "description": "通过事件列表查询接口,可以查出系统记录的7天内资源操作记录。",
      "inputSchema": {
        "type": "object",
        "properties": {
          "enterprise_project_id": {
            "description": "标示查询事件列表对应的企业项目ID。",
            "type": "string",
            "maxLength": 40,
            "minLength": 15,
            "in": "query"
          },
          "access_key_id": {
            "description": "标示查询事件列表对应的访问密钥ID。包含临时访问凭证和永久访问密钥。",
            "type": "string",
            "maxLength": 32,
            "minLength": 1,
            "in": "query"
          },
          "trace_rating": {
            "example": "normal",
            "description": "标示查询事件列表对应的事件等级目前有三种:正常(normal), 警告(warning),事故(incident)。",
            "type": "string",
            "maxLength": 10,
            "minLength": 5,
            "enum": [
              "normal",
              "warning",
              "incident"
            ],
            "in": "query"
          },
          "trace_name": {
            "example": "createTracker",
            "description": "标示查询事件列表对应的事件名称。",
            "type": "string",
            "maxLength": 100,
            "minLength": 1,
            "in": "query"
          },
          "trace_id": {
            "example": "e001ccb9-bc09-11e6-b00b-4b2a61338db6",
            "description": "标示某一条事件的事件ID。当传入这个查询条件时,其他查询条件自动不生效。",
            "type": "string",
            "maxLength": 200,
            "minLength": 1,
            "in": "query"
          },
          "resource_type": {
            "example": "eip",
            "description": "标示查询事件列表对应的资源类型。",
            "type": "string",
            "maxLength": 100,
            "minLength": 1,
            "in": "query"
          },
          "resource_name": {
            "example": "192.144.163.1",
            "description": "标示查询事件列表对应的的资源名称。",
            "type": "string",
            "maxLength": 300,
            "minLength": 1,
            "in": "query"
          },
          "resource_id": {
            "example": "d502809d-0d1d-41ce-9690-784282142ccc",
            "description": "标示查询事件列表对应的云服务资源ID。",
            "type": "string",
            "maxLength": 350,
            "minLength": 0,
            "in": "query"
          },
          "user": {
            "example": {
              "name": "xxx",
              "id": "a2e899190fcd444084a68fc0ac2sc1e9",
              "domain": {
                "name": "xxx",
                "id": "05b2598d69bc4a209f9ac5eeeb1f91ad"
              }
            },
            "description": "标识特定用户名称,用以查询该用户下的所有事件。",
            "type": "string",
            "maxLength": 1024,
            "minLength": 1,
            "in": "query"
          },
          "service_type": {
            "example": "CTS",
            "description": "标识查询事件列表对应的云服务类型。必须为已对接CTS的云服务的英文缩写,且服务类型一般为大写字母。",
            "type": "string",
            "maxLength": 32,
            "minLength": 1,
            "in": "query"
          },
          "tracker_name": {
            "example": "data-tracker-name",
            "description": "当\"trace_type\"字段值为\"system\"时,该字段值默认为\"system\"。",
            "type": "string",
            "maxLength": 64,
            "minLength": 1,
            "in": "query"
          },
          "to": {
            "example": 1479095278000,
            "description": "标识查询事件列表的结束时间戳(timestamp,为标准UTC时间,毫秒级,13位数字,不包括传入时间)默认为当前时间戳。查询条件to与from配套使用。",
            "type": "integer",
            "format": "int64",
            "maximum": 4102416000000,
            "minimum": 946656000000,
            "in": "query"
          },
          "next": {
            "example": "1478490478000",
            "description": "取值为响应中中marker的值,用于标识查询事件的起始时间(自此条事件的记录时间起,向更早时间查询)。",
            "type": "string",
            "maxLength": 50,
            "minLength": 10,
            "in": "query"
          },
          "from": {
            "example": 1478490478000,
            "description": "标识查询事件列表的起始时间戳(timestamp,为标准UTC时间,毫秒级,13位数字,不包括传入时间)默认为上一小时的时间戳。查询条件from与to配套使用。",
            "type": "integer",
            "format": "int64",
            "maximum": 4102416000000,
            "minimum": 946656000000,
            "in": "query"
          },
          "limit": {
            "example": 11,
            "description": "标示查询事件列表中限定返回的事件条数。不传时默认10条,最大值200条。",
            "type": "integer",
            "format": "int32",
            "maximum": 200,
            "minimum": 0,
            "in": "query"
          },
          "trace_type": {
            "example": "system",
            "description": "标识审计事件类型。",
            "type": "string",
            "maxLength": 32,
            "minLength": 1,
            "enum": [
              "system",
              "data"
            ],
            "in": "query"
          },
          "project_id": {
            "example": "2b31ed520xxxxxxebedb6e57xxxxxxxx",
            "description": "项目ID,参见《云审计服务API参考》“获取账号ID和项目ID”章节。",
            "type": "string",
            "maxLength": 40,
            "minLength": 15,
            "in": "path"
          }
        },
        "required": [
          "project_id",
          "trace_type"
        ]
      }
    }
  ]
}
