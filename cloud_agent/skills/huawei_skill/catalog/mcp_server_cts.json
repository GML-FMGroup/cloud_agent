    {
      "name": "CreateTracker",
      "description": "云审计服务开通后系统会自动创建一个追踪器,用来关联系统记录的所有操作。目前,一个云账户在一个Region下支持创建一个管理类追踪器和多个数据类追踪器。",
      "inputSchema": {
        "type": "object",
        "properties": {
          "project_id": {
            "example": "2b31ed520xxxxxxebedb6e57xxxxxxxx",
            "description": "项目ID,参见《云审计服务API参考》“获取账号ID和项目ID”章节。",
            "type": "string",
            "maxLength": 40,
            "minLength": 15,
            "in": "path"
          },
          "tracker_type": {
            "description": "标识追踪器类型。",
            "type": "string",
            "enum": [
              "system",
              "data"
            ]
          },
          "tracker_name": {
            "type": "string",
            "description": "标识追踪器名称。",
            "maxLength": 64,
            "minLength": 1,
            "example": "data-tracker-name"
          },
          "agency_name": {
            "type": "string",
            "description": "云服务委托名称。",
            "maxLength": 32,
            "minLength": 1,
            "example": "cts_admin_trust",
            "enum": [
              "cts_admin_trust"
            ]
          },
          "is_organization_tracker": {
            "type": "boolean",
            "description": "是否应用到我的组织。",
            "default": false
          },
          "management_event_selector": {
            "type": "object",
            "description": "管理类事件选择器。",
            "properties": {
              "exclude_service": {
                "type": "array",
                "description": "标识不转储的云服务名称。",
                "example": "",
                "items": {
                  "example": "KMS",
                  "type": "string",
                  "minLength": 1,
                  "maxLength": 20
                },
                "minItems": 0,
                "maxItems": 10
              }
            }
          },
          "is_lts_enabled": {
            "type": "boolean",
            "description": "是否打开事件分析。",
            "example": true
          },
          "obs_info": {
            "description": "转储桶配置",
            "properties": {
              "bucket_name": {
                "type": "string",
                "description": "标识OBS桶名称。由数字或字母开头,支持小写字母、数字、“-”、“.”,长度为3~63个字符。",
                "maxLength": 63,
                "minLength": 3,
                "example": "obs-f1da"
              },
              "file_prefix_name": {
                "type": "string",
                "description": "标识需要存储于OBS的日志文件前缀,0-9,a-z,A-Z,'-','.','_'长度为0~64字符。",
                "maxLength": 64,
                "minLength": 0,
                "example": "yO8Q"
              },
              "is_obs_created": {
                "type": "boolean",
                "description": "是否支持新建OBS桶。",
                "example": "true"
              },
              "bucket_lifecycle": {
                "type": "integer",
                "description": "标识配置桶内对象存储周期。",
                "minimum": 0,
                "maximum": 5000,
                "example": "30"
              },
              "compress_type": {
                "type": "string",
                "description": "压缩类型。包括不压缩(json),压缩(gzip)两种状态。默认为gzip格式。",
                "default": "gzip",
                "enum": [
                  "gzip",
                  "json"
                ]
              },
              "is_sort_by_service": {
                "type": "boolean",
                "description": "路径按云服务划分,打开后转储文件路径中将增加云服务名。默认为true。",
                "default": true
              }
            },
            "type": "object"
          },
          "is_support_trace_files_encryption": {
            "type": "boolean",
            "description": "事件文件转储加密功能开关。",
            "example": true
          },
          "kms_id": {
            "type": "string",
            "description": "事件文件转储加密所采用的秘钥id(从KMS获取)。",
            "maxLength": 200,
            "minLength": 0,
            "example": "13a4207c-7abe-4b68-8510-16b84c3b5504"
          },
          "is_support_validate": {
            "type": "boolean",
            "description": "事件文件转储时是否打开事件文件校验。",
            "example": true
          },
          "data_bucket": {
            "properties": {
              "data_bucket_name": {
                "type": "string",
                "maxLength": 63,
                "minLength": 3,
                "description": "数据类追踪器追踪对象的桶名。"
              },
              "data_event": {
                "type": "array",
                "maxItems": 2,
                "minItems": 1,
                "items": {
                  "type": "string",
                  "enum": [
                    "WRITE",
                    "READ"
                  ],
                  "example": "WRITE"
                },
                "description": "数据类追踪器追踪的操作类型。"
              }
            },
            "type": "object",
            "description": "追踪桶配置信息。"
          }
        },
        "required": [
          "project_id",
          "tracker_name",
          "tracker_type"
        ]
      }
    },
