    {
      "name": "DetectStandardByNameAndId",
      "description": "使用姓名、身份证号文本和人脸图片进行三要素身份审核。",
      "inputSchema": {
        "type": "object",
        "properties": {
          "meta": {
            "properties": {
              "uuid": {
                "required": true,
                "description": "唯一标识此次请求的ID,用户自定义,不超过64位。",
                "type": "string"
              }
            },
            "type": "object",
            "description": "请求元数据,例如表征本次请求的唯一标注uuid。"
          },
          "data": {
            "description": "请求消息的数据部分。",
            "properties": {
              "req_data": {
                "description": "请求列表,用于支持批量调用。目前暂时只支持单个数据查询。",
                "type": "array",
                "items": {
                  "properties": {
                    "verification_name": {
                      "description": "被验证人的姓名。",
                      "type": "string"
                    },
                    "verification_id": {
                      "description": "被验证人的身份证号码。",
                      "type": "string"
                    },
                    "face_image": {
                      "description": "现场人像图像数据,使用base64编码,要求base64编码后大小不超过4M。图像各边的像素大小在300到4000之间,支持JPG格式。",
                      "type": "string"
                    },
                    "detail": {
                      "description": "响应参数similarity是否详细显示,默认为false。",
                      "type": "boolean"
                    },
                    "crop": {
                      "description": "是否允许对入参face_image进行人脸检测及图片裁剪,默认为true,表示允许。",
                      "type": "boolean"
                    }
                  },
                  "required": [
                    "verification_name",
                    "verification_id",
                    "face_image"
                  ],
                  "type": "object"
                }
              }
            }
          }
        },
        "required": [
          "data",
          "meta"
        ]
      }
    },
